	.container.mobile
		.utilities-list.row
			.col-12
				.img
					img(src="./assets/index/i_5.jpg", alt="")
			-for(let i =1; i<13; i++)
				.col-6.col-sm-4
					a(href="./assets/utilities/"+i+".jpg" data-fancybox="utilities_"+i)
