.section#sec-5(fp-title="Tiện ích")
	.desktop.desktop-content
		.swiper-prev.desktop
			img.lazyload(data-src="./assets/btn_toggle.png", alt="")
		.swiper-next.desktop
			img.lazyload(data-src="./assets/btn_toggle.png", alt="")
		.swiper-container
			.swiper-wrapper
				.swiper-slide.imagemap-with-list.slide-1
					.imgMapCanvas(data-canvas="label")
						.map-canvas
							map(name="imgMap_3", id="imgMap_3")
								<area shape="poly" alt="1" coords="1315, 620, 1255, 629, 1283, 672, 1337, 661" />
